<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ImageActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#ededed">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_protein_facts"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn1" />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_food_supplements"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn2"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_rejuvenate_with_detox"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn3"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_liquid_nutritional_drinks"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn4"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_commom_eating_mistakes"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn5"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/dashboard_nutrition_weight_loss_formula"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/fn6"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:cardElevation="3dp"
            app:cardCornerRadius="10dp">

